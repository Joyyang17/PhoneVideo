<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_event_bus"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.joy.phonevideo.activity.EventBusActivity">

    <Button
        android:text="跳转到发送页面"
        android:id="@+id/bt_eventbus_send"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <Button
        android:text="粘性事件跳转到发送页面"
        android:id="@+id/bt_eventbus_sendAnd"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <TextView
        android:textColor="@color/black"
        android:textSize="30sp"
        android:id="@+id/tv_eventbus_reseive"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</LinearLayout>
